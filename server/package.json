{
  "name": "lensfinder-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch --clear-screen=false index.ts",
    "build": "tsc -p tsconfig.json",
    "openapi": "cross-env NODE_ENV=development tsx scripts/generate-openapi.ts",
    "graphql:schema": "cross-env NODE_ENV=development tsx scripts/generate-graphql-sdl.ts",
    "pretest": "npm run openapi",
    "test": "vitest --run && tsc -p tsconfig.json --noEmit",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "graphql": "16.9.0",
    "graphql-http": "1.22.1",
    "cheerio": "1.0.0",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "5.1.0",
    "compression": "1.7.4",
    "node-fetch": "3.3.2",
    "pg": "8.12.0",
    "helmet": "7.1.0",
    "express-rate-limit": "7.4.0",
    "pino": "9.3.2",
    "pino-http": "10.3.0",
    "prom-client": "15.1.3",
    "graphql-depth-limit": "1.1.0",
    "graphql-query-complexity": "0.12.0",
    "swagger-ui-dist": "5.17.14",
    "redoc": "2.4.0",
    "ioredis": "5.4.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.3",
    "@types/node": "22.5.4",
    "@types/pg": "^8.15.5",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/supertest": "^6.0.3",
    "cross-env": "7.0.3",
    "@apidevtools/swagger-parser": "10.1.0",
    "openapi-response-validator": "12.1.3",
    "supertest": "7.1.3",
    "ts-node": "10.9.2",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vitest": "3.2.4"
  }
}
