{
  "name": "camera-system-builder-client",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "predev": "kill-port ${CLIENT_PORT:-3000} || true",
    "dev": "vite --port ${CLIENT_PORT:-3000} --strictPort",
    "build": "vite build",
    "test": "vitest --run --coverage",
    "start": "vite preview --port ${CLIENT_PORT:-3000} --strictPort",
    "preview": "vite preview --port 4173",
    "openapi:types": "openapi-typescript ../server/openapi.json -o src/types/openapi.d.ts",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "pretest": "npm run openapi:types && node -e \"require('fs').mkdirSync('coverage/.tmp',{recursive:true})\"",
    "prebuild": "(cd ../server && npm run graphql:schema || true) && npm run openapi:types && (npm run graphql:codegen || true)"
  },
  "dependencies": {
    "@radix-ui/react-slider": "^1.3.5",
    "@tanstack/react-table": "^8.20.0",
    "plotly.js-dist-min": "2.35.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-plotly.js": "^2.6.0",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "10.4.20",
    "framer-motion": "^11.18.2",
    "jsdom": "24.1.0",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.9",
    "typescript": "5.5.4",
    "vite": "5.3.4",
    "vitest": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "openapi-typescript": "7.4.3",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.5.0"
  }
,
  "browserslist": [
    "defaults",
    "not IE 11",
    "not dead"
  ]
}
